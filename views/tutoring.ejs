  <section id="pricing">

    <h2>Services and Rates</h2>
    <p>Choose the study plan that works for you.</p>
    <p>Payments by e-transfer to carlos.restrepo@live.ca</p>

    <div class="row">
      <div class="pricing-col col-lg-4 col-md-6">
        <div class="card h-100">
          <div class="card-header">
            <h3>First Session</h3>
          </div>
          <div class="card-body d-flex flex-column">
            <h3>$50/hr</h3>
            <p>At least 48hr advance notice, $60/hr with short notice.</p>
            <a class="mt-auto" href="#schedule"><button class="btn btn-outline-dark btn-lg w-100 mt-auto" id="firstSessionButton" type="button">Book</button></a>
          </div>
        </div>
      </div>

      <div class="pricing-col col-lg-4 col-md-6">
        <div class="card h-100">
          <div class="card-header">
            <h3>Single Booking</h3>
          </div>
          <div class="card-body d-flex flex-column">
            <h3>$150 for two hours.</h3>
            <p>$80 for one hour.</p>
            <p>At least 48hr advance notice, $160 for two and $90 one with short notice.</p>
            <a class="mt-auto" href="#schedule"><button class="btn btn-dark btn-lg w-100 mt-auto" id="singleSessionButton" type="button">Book</button></a>
          </div>
        </div>
      </div>

      <div class="pricing-col col-lg-4">
        <div class="card h-100">
          <div class="card-header">
            <h3>Four Hour Deal</h3>
          </div>
          <div class="card-body d-flex flex-column">
            <h3>$70/hr</h3>
            <p>$280 total</p>
            <p>Time and date chosen at time of booking.</p>
            <p>Any changes to time and date must have at least 48hr advance notice and subject to availability.</p>
            <p>$40 to book, $60/hr before the day of session.</p>
            <a class="mt-auto" href="#schedule"><button class="btn btn-dark btn-lg w-100 mt-auto" id="fourHourButton" type="button">Book</button></a>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="pricing-col col-lg-12">
        <div class="card h-100">
          <div class="card-header">
            <h3>Rest of Semester Deal</h3>
          </div>
          <div class="card-body d-flex flex-column">
            <h3>$65/hr</h3>
            <p>Must book at least one hour per week until the end of the semester</p>
            <p>Time and date chosen at time of booking.</p>
            <p>Any changes to time and date must have at least 48hr advance notice and subject to availability.</p>
            <p>$10/hr to book, $55/hr before the day of the session.</p>
            <a class="mt-auto" href="#schedule"><button class="btn btn-dark btn-lg w-100 mt-auto" id="semesterDealButton" type="button">Book</button></a>
          </div>
        </div>
      </div>
    </div>
  </section>








  <section id='schedule'>
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-5">
          <h2>Book Now!</h2>
          <form action="/tutoring/request" method="post">
            <div class="row">
              <div class="form-group col-lg-6">
                <label for="bookFirstName">FirstName:</label>
                <input type="text" class="form-control" id="bookFirstName" name="bookFirstName" placeholder="First Name">
              </div>

              <div class="form-group col-lg-6">
                <label for="bookLastName">LastName:</label>
                <input type="text" class="form-control" id="bookLastName" name="bookLastName" placeholder="Last Name">
              </div>

              <div class="form-group">
                <label for="bookEmail">Email:</label>
                <input type="email" class="form-control" id="bookEmail" name="bookEmail" placeholder="Email">
              </div>
            </div>

            <div class="row">
              <div class="col-lg-4">
                <label for="bookDate">Date:</label>
                <input type="date" class="form-control" id="bookDate" name="bookDate">
              </div>


              <div class="col-lg-4">
                <label for="bookStart">Start:</label>
                <select id="bookStart" class="form-select" name="bookStart">
                  <option value=9 selected>9am</option>
                  <option value=10>10am</option>
                  <option value=11>11am</option>
                  <option value=12>12pm</option>
                  <option value=13>1pm</option>
                  <option value=14>2pm</option>
                  <option value=15>3pm</option>
                  <option value=16>4pm</option>
                  <option value=17>5pm</option>
                </select>
                <!-- <input type="time" class="form-control" id="bookStart" name="bookStart"> -->
              </div>


              <div class="col-lg-4">
                <label for="bookEnd">End:</label>
                <select id="bookEnd" class="form-select" name="bookEnd">
                  <option value=10 selected>10am</option>
                  <option value=11>11am</option>
                  <option value=12>12pm</option>
                  <option value=13>1pm</option>
                  <option value=14>2pm</option>
                  <option value=15>3pm</option>
                  <option value=16>4pm</option>
                  <option value=17>5pm</option>
                  <option value=18>6pm</option>
                </select>
                <!-- <input type="time" class="form-control" id="bookEnd" name="bookEnd"> -->
              </div>
            </div>

            <div class="row">
              <div class="form-group col-lg-4">
                <label for="bookDelivery">Delivery:</label>
                <select id="bookDelivery" class="form-select" name="bookDelivery">
                  <option value="O" selected>Online</option>
                  <option value="I">In-Person</option>
                </select>
              </div>

              <div class="form-group col-lg-4">
                <label for="bookCourse">Course:</label>
                <input type="text" class="form-control" id="bookCourse" name="bookCourse" placeholder="Course">
              </div>

              <div class="form-group col-lg-4">
                <label for="bookUniversity">University:</label>
                <input type="text" class="form-control" id="bookUniversity" name="bookUniversity" placeholder="University">
              </div>
            </div>



            <div class="form-row">
              <div class="form-group">
                <label for="bookComments">Additional comments:</label>
                <textarea class="form-control" id="bookComments" name="bookComments"></textarea>
              </div>
            </div>

            <button type="submit" id="requestBookingButton" class="btn btn-primary booking-button">Request Booking</button>
          </form>
        </div>
        <div class="col-lg-7">
          <h3>Availability</h3>

          <iframe src="https://calendar.google.com/calendar/embed?height=675&wkst=1&bgcolor=%23ffffff&ctz=America%2FToronto&showTitle=0&showNav=1&showPrint=0&showTabs=1&mode=WEEK&showCalendars=0&showTz=0&src=MDdhYWU3MTQ1ODYzN2FmN2E5MTk3YjMzN2JkMTczMWJlMzc1M2IzMDZhMDAxZjRjZDJiZGYyY2VhODdhMGQ0OEBncm91cC5jYWxlbmRhci5nb29nbGUuY29t&src=N2EzMWI2MGFkYTQ4MDM4ZTM1M2VmZWEwZDI1YTI1OWVhZWI1MTU4NjNjNzQ0ZTQzNDAyZjJlYTkyOTE3ZDFlNkBncm91cC5jYWxlbmRhci5nb29nbGUuY29t&color=%23616161&color=%23AD1457" style="border:solid 1px #777" width="1000" height="675" frameborder="0" scrolling="no"></iframe>
        </div>

      </div>



    </div>
  </section>




